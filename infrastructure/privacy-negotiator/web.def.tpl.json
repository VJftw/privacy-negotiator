[
  {
    "name": "web_${environment}",
    "image": "vjftw/privacy-negotiator:web-${version}",
    "essential": true,
    "memoryReservation": 128,
    "portMappings": [
      {
        "containerPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${environment}.web-container-logs",
        "awslogs-region": "${cloudwatch_region}",
        "awslogs-stream-prefix": "web"
      }
    },
    "environment": [
      {
        "Name": "WEAVE_CIDR",
        "Value": "net:${weave_cidr}"
      },
      {
        "Name": "API_ENDPOINT",
        "Value": "${api_endpoint}"
      },
      {
        "Name": "FB_APP_ID",
        "Value": "${fb_app_id}"
      },
    ]
  }
]
